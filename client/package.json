{
  "name": "mern-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@headlessui/react": "^1.7.7",
    "@stripe/stripe-js": "^1.46.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.6.1",
    "react-scripts": "^5.0.1",
    "use-local-storage": "^2.3.6"
  },
  "scripts": {
    "start": "npm run watch:css && if-node-version '>= 17' && react-scripts --openssl-legacy-provider start || react-scripts start",
    "build": "npm run build:css && if-node-version '>= 17' && react-scripts --openssl-legacy-provider build || react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "build:css": "postcss src/index.css -o src/index.css",
    "watch:css": "postcss src/index.css -o src/index.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.2.4"
  }
}
